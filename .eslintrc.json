{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard-with-typescript"
    ],
    "parser": "@typescript-eslint/parser",
    "overrides": [
    ],
    "parserOptions": { "project": ["./tsconfig.json"] },
    "plugins": [
        "react"
    ],
    "rules": {
        "dot-notation": "off",
        "@typescript-eslint/no-misused-promises": "off"
    }
}
